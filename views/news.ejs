<%- include ("./partials/header")%>
<div class="container">
    <div class="row">
        <h1 style="text-align: center">Create a New Patient</h1>
        <div style="width: 30%; margin: 25px auto;">
            <form action="/adminp" method="POST">

                <div class="form-group">
                    <input class="form-control" type="text" name="name" placeholder="name">
                </div>


                <div class="form-group">
                    <input class="form-control" type="text" name="age" placeholder="Age">
                </div>



                <div class="form-group">
                    <input class="form-control" type="text" name="sex" placeholder="SEX">
                </div>


                 <div class="form-group">
                    <input class="form-control" type="text" name="Blood_group" placeholder="bloodgroup">
                </div>


                 <div class="form-group">
                    <input class="form-control" type="tel" name="Disease" placeholder="Disease">
                </div>


                 <div class="form-group">
                    <input class="form-control" type="Number" name="requirement" placeholder="requirement">
                </div>


                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block">Submit!</button>
                </div>
            </form>
            <a href="/adminp">Go Back</a>
        </div>
    </div>
</div>
<%- include ("./partials/footer")%>